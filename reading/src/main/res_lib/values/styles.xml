<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- BezelImageView -->
    <style name="BezelImageView">
        <item name="biv_maskDrawable">@drawable/material_drawer_circle_mask</item>
        <item name="biv_drawCircularShadow">true</item>
        <item name="biv_selectorOnPress">@color/material_drawer_primary</item>
    </style>

    <style name="pickerview_dialogAnim" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/pickerview_dialog_scale_in</item>
        <item name="android:windowExitAnimation">@anim/pickerview_dialog_scale_out</item>
    </style>
</resources>